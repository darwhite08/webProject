<%- include('main-comp/header') %>
<!-- Home Tab -->
<% if (tab === "home") { %>
    <%- include('navigations/topBar' ,{value:false}) %>
    <%- include('home') %> 
    <%- include('navigations/bottomBar' ) %>
    <%- include('navigations/mobile-navigation' ) %> 
    <!-- Blog Tab -->
    <% } else if (tab === "blog"){ %>
        <%- include('navigations/topBar' ,{value:userTrue}) %>
        <%- include('anime-vault/blog') %> 
        <%- include('navigations/bottomBar' ) %>
        <!-- Count Down Tab -->
        <% } else if (tab === "countdown"){ %>
            <%- include('navigations/topBar' ,{value:userTrue}) %>
            <%- include('anime-vault/animeCountDown') %>
            <%- include('navigations/bottomBar' ) %>
            <%- include('navigations/mobile-navigation' ) %> 
            <!-- bestAnimeList Tab -->
        <% } else if (tab==="bestanimelist"){ %>
            <%- include('navigations/topBar' ,{value:userTrue}) %>
            <%- include('anime-vault/bestAnimeList') %>
            <%- include('navigations/bottomBar' ) %>
            <%- include('navigations/mobile-navigation' ) %>  
         
            
                <% } %> 

<%- include('main-comp/footer') %>
